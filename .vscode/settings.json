{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "files.associations": {
        "vector": "cpp",
        "iostream": "cpp",
        "iosfwd": "cpp",
        "chrono": "cpp",
        "random": "cpp",
        "limits": "cpp",
        "algorithm": "cpp",
        "thread": "cpp",
        "*.tcc": "cpp",
        "shared_mutex": "cpp",
        "functional": "cpp",
        "array": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "utility": "cpp",
        "unordered_set": "cpp",
        "cstddef": "cpp",
        "istream": "cpp",
        "iterator": "cpp",
        "set": "cpp",
        "ostream": "cpp",
        "optional": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "memory": "cpp",
        "fstream": "cpp",
        "sstream": "cpp",
        "streambuf": "cpp",
        "tuple": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "cstdlib": "cpp",
        "cmath": "cpp",
        "cassert": "cpp"
    },
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.clang.executable": "clang-10",
    "c-cpp-flylint.clang.standard": [
        "c++20"
    ],
    "c-cpp-flylint.cppcheck.standard": [
        "c++20"
    ],
    "c-cpp-flylint.clang.extraArgs": [
        "-Wno-c++98-compat"
    ],
}